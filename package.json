{
  "name": "@chinaza/honey",
  "version": "1.28.1",
  "description": "An expressJS based library for simplifying the development of CRUD APIs using Postgres",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/module.js",
  "source": "src/index.ts",
  "author": "chinaza",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=20.9.0"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/chinaza/honey.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "parcel build",
    "build:dev": "parcel watch",
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "test": "jest --passWithNoTests",
    "lint-staged": "lint-staged",
    "test-watch": "jest --watch --passWithNoTests",
    "start": "node dist/index.js",
    "start:dev": "nodemon --exec ts-node --transpile-only ./src/index.ts",
    "prepare": "npm run build",
    "prepare:dev": "husky install",
    "bump": "yarn changeset; yarn changeset version"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "prettier": "@chinaza/eslint-config/prettier",
  "eslintConfig": {
    "extends": [
      "@chinaza/eslint-config/express"
    ]
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@chinaza/eslint-config": "^1.2.3",
    "@parcel/compressor-brotli": "^2.13.3",
    "@parcel/compressor-gzip": "^2.13.3",
    "@parcel/packager-ts": "^2.13.3",
    "@parcel/transformer-typescript-types": "^2.13.3",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.5",
    "@types/yamljs": "^0.2.34",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.3",
    "parcel": "^2.13.3",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.2",
    "express": "^4.21.2",
    "express-oas-generator": "^1.0.46",
    "joi": "^17.12.0",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "yamljs": "^0.3.0"
  }
}
