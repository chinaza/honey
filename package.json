{
  "name": "@chinaza/honey",
  "version": "1.1.0",
  "description": "An expressJS based library for simplifying the development of CRUD APIs using Postgres",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/module.js",
  "source": "src/index.ts",
  "author": "chinaza",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=14.0.0"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/chinaza/honey.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "parcel build",
    "build:dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "test": "jest --passWithNoTests",
    "lint-staged": "lint-staged",
    "test-watch": "jest --watch --passWithNoTests",
    "start": "node dist/index.js",
    "start:dev": "nodemon --exec ts-node --transpile-only ./src/index.ts",
    "prepare": "npm run build",
    "prepare:dev": "husky install"
  },
  "alias": {
    "@src/*": "./src/$1"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "prettier": "@chinaza/eslint-config/prettier",
  "eslintConfig": {
    "extends": [
      "@chinaza/eslint-config/express"
    ]
  },
  "devDependencies": {
    "@chinaza/eslint-config": "^1.0.5",
    "@parcel/compressor-brotli": "^2.5.0",
    "@parcel/compressor-gzip": "^2.5.0",
    "@parcel/packager-ts": "2.5.0",
    "@parcel/transformer-typescript-types": "2.5.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.7",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "eslint": "^8.6.0",
    "husky": "^8.0.0",
    "jest": "^27.4.5",
    "lint-staged": "^12.4.1",
    "nodemon": "^2.0.15",
    "parcel": "^2.5.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@types/module-alias": "^2.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "joi": "^17.5.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.19.2",
    "yamljs": "^0.3.0"
  }
}
